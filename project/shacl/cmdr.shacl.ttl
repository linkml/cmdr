@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://example.org/TEMP/Database> a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://example.org/TEMP/Subject> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path <https://example.org/TEMP/subjects> ] ;
    sh:targetClass <https://example.org/TEMP/Database> .

<https://example.org/TEMP/Diagnosis> a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path <https://example.org/TEMP/source> ],
        [ sh:class <https://example.org/TEMP/Condition> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path <https://example.org/TEMP/is_about> ] ;
    sh:targetClass <https://example.org/TEMP/Diagnosis> .

<https://example.org/TEMP/Document> a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path <https://example.org/TEMP/is_about> ] ;
    sh:targetClass <https://example.org/TEMP/Document> .

