<https://example.org/TEMP/Specimen> a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://example.org/TEMP/ResearchSubject> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path <https://example.org/TEMP/has_source> ],
        [ sh:class <https://example.org/TEMP/Specimen> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path <https://example.org/TEMP/has_parent_specimen> ] ;
